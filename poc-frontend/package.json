{
  "name": "DigitalStreamingApplication",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@mui/x-date-pickers": "^5.0.7",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.6",
    "framer-motion": "^7.1.0",
    "mui-datatables": "^4.2.2",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-player": "^2.10.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.1",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.4.2",
    "use-immer": "^0.7.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage --watchAll",
    "test-coverage": "react-scripts test --coverageDirectory test-coverage --transformIgnorePatterns \"node_modules/(?!axios)/\" --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": -10
      }
    }
  },
  "jestSonar": {
    "reportPath": "test-coverage/sonar",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "devDependencies": {
    "@types/mui-datatables": "^3.7.9",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.10"
  }
}
